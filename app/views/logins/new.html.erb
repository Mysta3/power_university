<div class="row">
  <div class="col s12 center-align">
    <h3>Login</h3>
  </div>
</div>

<div class="container">
  <div class="row">
    <%= form_for(:logins, html: {class: "col s12"}) do |f| %>

    <div class="row">
      <div class="input-field col s12">
        <i class="material-icons prefix">email</i>
        <%= f.email_field :email, placeholder: "Enter Your Email" , autofocus: true %>
      </div>
    </div>
    <div class="row">
      <div class="input-field col s12">
        <i class="material-icons prefix">fingerprint</i>
        <%= f.password_field :password, placeholder: "Enter Your Password" %>
      </div>
    </div>
    <%= button_tag( class:"btn waves-effect waves-light right") do %> Login
    <i class="material-icons right">send</i>
    <% end %>
    <% end %>
  </div>
</div>